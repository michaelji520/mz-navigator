{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "module": "es6", // 编译出的代码采用的模块规范
    "target": "es5", // 编译出的代码采用 ES 的哪个版本
    "sourceMap": true, // 输出 Source Map 方便调试
    "types": ["node"]
  },
  "include": ["src/**/*", "src/node_modules"],
  "exclude": [ // 不编译这些目录里的文件
    "node_modules"
  ]
}